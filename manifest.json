{
    "manifest_version": 2,
    "name": "Google Shortcuts",
    "description":"Now you can learn Google shortcuts",
    "version": "1.0",
    "icons": {
        "128": "L1.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": 
    {
        "default_icon": "L1.png",
        "default_popup": "popup.html",
        "default_title": "Learn Google Chrome Shortcuts with this popup."
    },
    "permissions": ["activeTab"]
}